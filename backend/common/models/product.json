{
  "name": "Product",
  "plural": "Products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "recordCreated": {
      "type": "date",
      "default": "$now"
    },
    "durationFrom": {
      "type": "date"
    },
    "durationTo": {
      "type": "date"
    },
    "price": {
      "type": "number"
    },
    "label": {
      "type": "string"
    },
    "refTimeUnit": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "refOwnerUserId": {
      "type": "number"
    },
    "state": {
      "type": "number"
    },
    "refTimeOut": {
      "type": "number"
    },
    "refCityId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "userProducts": {
      "type": "hasMany",
      "model": "UserProduct",
      "foreignKey": "refProductId",
      "through": "UserProduct"
    },
    "productProductCategories": {
      "type": "hasMany",
      "model": "Product_ProductCategory",
      "foreignKey": "refProductId",
      "through": "Product_ProductCategory"
    },
    "timeUnits": {
      "type": "hasOne",
      "model": "TimeUnit",
      "foreignKey": "refTimeUnit"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
